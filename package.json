{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.6.0",
    "dotenv": "^10.0.0",
    "mocha": "^9.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dotenv": "^0.1.3",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^3.3.0",
    "selenium-webdriver": "^4.0.0-rc-1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "react-dotenv && react-scripts start",
    "build": "react-dotenv && react-scripts build",
    "serve": "react-dotenv && serve build",
    "test": "react-scripts test --env=jsdom",
    "testselenium": "node ./node_modules/mocha/bin/mocha --recursive tests --timeout 20000"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "chromedriver": "^93.0.1",
    "mocha": "^9.1.1",
    "web-vitals": "^2.1.0"
  },
  "react-dotenv": {
    "whitelist": [
      "production_API_URL",
      "development_API_URL"
    ]
  }
}
